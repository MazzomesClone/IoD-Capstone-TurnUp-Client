{
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "${VITE_API_URL}/api/:match*"
        },
        {
            "source": "/public/:match*",
            "destination": "${VITE_API_URL}/public/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}