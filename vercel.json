{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "vite.config.js"
        },
        {
            "src": "/public/(.*)",
            "dest": "vite.config.js"
        }
    ]
}